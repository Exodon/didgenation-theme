# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = tab
trim_trailing_whitespace = true
max_line_length = off

[package.json]
indent_style = tab
indent_size = 2

[*.scss]
indent_style = tab
indent_size = 2

[*.html]
indent_style = tab
indent_size = 2

[*.xml]
indent_style = tab
indent_size = 2

